{
    "name": "opensalt/opensalt",
    "license": "MIT",
    "type": "project",
    "description": "A competency framework management system that works with the CASE format",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "exclude-from-classmap": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Codeception\\Module\\": "tests/_support/Module"
        },
        "classmap": [
            "tests/_behat/Context.php"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/opensalt/OpcacheBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/PCGGenEd/faker-context"
        },
        {
            "type": "vcs",
            "url": "https://github.com/opensalt/EntityAuditBundle"
        },
        {
            "type": "package",
            "package": {
                "name": "firebase",
                "version": "4.8.2",
                "dist": {
                    "url": "https://www.gstatic.com/firebasejs/4.8.2/firebase.js",
                    "type": "file"
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4.0",
        "ext-bz2": "*",
        "ext-ctype": "*",
        "ext-filter": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-PDO": "*",
        "antishov/doctrine-extensions-bundle": "^1.4.0",
        "colinodell/json5": "^2.1",
        "csa/guzzle-bundle": "^3.1.1",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.6",
        "gbprod/uuid-normalizer": "^1.1",
        "google/recaptcha": "~1.1",
        "jms/serializer-bundle": "^3.3.0",
        "kamermans/guzzle-oauth2-subscriber": "^1.0.6",
        "kreait/firebase-php": "^5.1",
        "laminas/laminas-code": "^3.3.1",
        "laminas/laminas-escaper": "^2.2",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/oauth2-github": "^2.0.0",
        "league/uri": "^6.0",
        "milo/github-api": "^1.4",
        "namshi/cuzzle": "^2.0",
        "nelmio/cors-bundle": "^2.0",
        "nelmio/security-bundle": "^2.7",
        "phpoffice/phpspreadsheet": "~1.14.0",
        "phpoffice/phpword": "^0.17.0",
        "qandidate/toggle-bundle": "dev-master#48a5f2dee81d51f6dd03d14753d423a65a6e2311",
        "ramsey/uuid": "^4.0",
        "ramsey/uuid-doctrine": "^1.6",
        "sensio/framework-extra-bundle": "^5.3",
        "simplethings/entity-audit-bundle": "dev-fixes-1.0.9-object-instead-of-string as 1.0.9.2",
        "swaggest/json-schema": "^0.12.20",
        "symfony/asset": "5.1.*",
        "symfony/console": "5.1.*",
        "symfony/expression-language": "5.1.*",
        "symfony/flex": "^1.8.4",
        "symfony/form": "5.1.*",
        "symfony/framework-bundle": "5.1.*",
        "symfony/mime": "5.1.*",
        "symfony/monolog-bundle": "^3.3",
        "symfony/process": "5.1.*",
        "symfony/profiler-pack": "^1.0.4",
        "symfony/proxy-manager-bridge": "5.1.*",
        "symfony/security-bundle": "5.1.*",
        "symfony/security-csrf": "5.1.*",
        "symfony/serializer-pack": "^1.0.2",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "5.1.*",
        "symfony/twig-pack": "^1.0.0",
        "symfony/validator": "5.1.*",
        "symfony/web-link": "5.1.*",
        "symfony/yaml": "5.1.*",
        "tecnickcom/tcpdf": "^6.2",
        "tetranz/select2entity-bundle": "^3.0.0",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/debug-pack": "*",
        "symfony/test-pack": "*",
        "symfony/maker-bundle": "^1.5",
        "symfony/phpunit-bridge": "5.1.*",

        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.0.0",
	"codeception/module-doctrine2": "^1.0.0",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-rest": "^1.0.0",
        "codeception/module-sequence": "^1.0.0",
        "codeception/module-symfony": "^1.0.0",
        "codeception/module-webdriver": "^1.0.0",
            "codeception/specify": "^1.1",
            "codeception/verify": "^1.0",
            "flow/jsonpath": "~0.4",
            "league/factory-muffin": "~3.0",
            "league/factory-muffin-faker": "~2.0",
            "phpunit/phpunit": "~7.5",
            "codeception/c3": "2.*",
            "vlucas/phpdotenv": "^5.0",

        "phpstan/phpstan-symfony": "^0.12.3",
        "phpstan/phpstan-doctrine": "^0.12.9",

        "vimeo/psalm": "^3.8.3",
        "weirdan/doctrine-psalm-plugin": "^0.11.0",

        "symplify/easy-coding-standard": "^8.0",
        "escapestudios/symfony2-coding-standard": "^3.1",
        "ericmartel/codeception-email-mailtrap": "^1.0",
        "theofidry/psysh-bundle": "^4.0.0"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.1.*"
        }
    }
}
